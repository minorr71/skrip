#include <iostream>
#include <cstdlib>
#include <ctime>

int main() {
    // Seed the random number generator
    srand(time(nullptr));

    // Generate a random number between 1 and 100
    int randomNumber = rand() % 100 + 1;

    // Print the random number
    std::cout << "Random number: " << randomNumber << std::endl;

    return 0;
}
